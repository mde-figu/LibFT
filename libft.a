#include "libft.h"

void        *ft_memchr(const void *s, int c, size_t len)
{
    unsigned char   *p= (unsigned char*)s;
    unsigned char   *fdchar = NULL;

    while ((s != NULL ) && (len--))
    {
        if (*p != (unsigned char*)c)
        {
            p = p + 1;
        }
        else
        {
            fdchar = p;
            break;
        }
    }
    return (fdchar);
}
