#include "libft.h"

char	*ft_strrchr(const char *s, int c)
{
	int i;
	const char *len;

	len = s;
	i = ft_strlen(s);
	s = (s + i);
	while (*s != *len && c != *s)
		s = s - 1;
	if (c == *s)
		return ((char *)s);
	return (0);
}
