#include "libft.h"

char		*ft_strtrim(char const *s1, char const *set)
{
	size_t i;

	i = ft_strlen(s1);
	if (s1 == NULL || set == NULL)
		return (0);
	while (ft_strchr(set, *s1) && *s1)
		s1 = s1 + 1;
	while (ft_strchr(set, s1[i]) && i)
		i = i - 1;
	return (ft_substr(s1, 0, i + 1));
}
